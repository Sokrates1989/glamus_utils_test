{
   "name": "glamus/utils",
    "description": "Utility class for glamus",
    "keywords": [
        "Utilities"
    ],
    "homepage": "https://github.com/glamus/utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Pan",
            "email": "peter@pan.de.me",
            "homepage": "http://peterpan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Glamus\\Utils\\": "src"
        }
